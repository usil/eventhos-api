{
  "state": "${NODE_ENV}",
  "port": "${PORT}",
  "cpuCount": "${CPU_COUNT}",
  "logs": {
    "useFile": true,
    "level": "debug"
  },
  "dataBase": {
    "name": "${DATA_BASE_NAME}",
    "host": "${DATA_BASE_HOST}",
    "port": "${DATA_BASE_PORT}",
    "user": "${DATA_BASE_USER}",
    "password": "${DATA_BASE_PASSWORD}",
    "acquireConnectionTimeout": "${DATA_BASE_ACQUIRE_CONNECTION_TIMEOUT}",
    "poolMax": "${DATA_BASE_POOL_MAX}",
    "poolMin": "${DATA_BASE_POOL_MIN}"
  },
  "subscription": {
    "timeout": 70000
  },
  "oauth2": {
    "jwtTokenExpiresIn": "24h",
    "jwtSecret": "${JWT_SECRET}"
  },
  "encryption": {
    "key": "${CRYPTO_KEY}"
  },
  "server": {
    "httpBodySizeLimit": "${HTTP_BODY_SIZE_LIMIT}"
  },
  "queue": {
    "active": "${USE_QUEUE}",
    "host": "${QUEUE_HOST}",
    "headersHost": "${QUEUE_HEADERS_HOST}",
    "port": "${QUEUE_PORT}",
    "user": "${QUEUE_USER}",
    "password": "${QUEUE_PASSWORD}",
    "destination": "${QUEUE_DESTINATION}",
    "heartBeat": "${QUEUE_HEART_BEAT}"
  }
}
